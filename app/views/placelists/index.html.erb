<h1>All Placelists</h1>
 <%= link_to 'Add a new Placelist', new_placelist_path %>

<div id="placelists">
  <% @placelists.each do |placelist| %>
    <div>
      <h2><%= link_to placelist.title, placelist_path(placelist) %></h2>
      <p><%= placelist.description %></p>
      <%= link_to 'Edit', edit_placelist_path(placelist) %>
      <%= link_to 'Delete', placelist_path(placelist), method: :delete %></p>
    </div>
  <% end %>
</div>
