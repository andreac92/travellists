<% content_for :pageTitle do %>
<div class="page-header">
  <h1 class="text-center">All Travelists</h1>
</div>
<% end %>
<div class="row">
	<div class="col-md-12">
		<table class="table">
			<tr>
				<th>ID</th>
				<th>Name</th>
				<th>Owner</th>
			</tr>
		  <% @travelists.each do |travelist| %>
		    <tr>
		      <td><%= travelist.id %></td>
		      <td><%= travelist.title %></td>
		      <td><%= travelist.user.name if travelist.user %></td>
		    </tr>
		  <% end %>
		</table>
	</div>
</div>